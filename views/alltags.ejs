<div id="username" style="display:none"><%= user ? user.username : "" %></div>
<div id="isPublic" style="display:none"><%= user ? user.isPublic:"" %></div>
<div style="width:900px; margin-top:50px">
<div class="input">
    <font style="margin:3px;">Jump to tag</font> <input id="Inputs" class="xlarge" style="margin:2px;" name="name" size="30" type="search" onkeydown='if(event.keyCode==13){jumpToTag("<%= (user&&user.isPublic=='false') ? "/username/" + user.username : "" %>");};'>
    <button class="btn btn-primary" onclick='jumpToTag("<%= (user&&user.isPublic=='false') ? "/username/" + user.username : "" %>");'>Go</button>
</div>
<br/>
<div style="font-weight:bolder">Top tags:</div>
<div class="dialog-createNew" style="display: block;z-index:9999;">
    <div class="tabbable" id="tabsNew">
        <ul class="nav nav-tabs">
            <li class="active">
                <a href="#tabs-1" data-toggle="tab">
                    order by number
                </a>
            </li>
            <li>
                <a href="#tabs-2" data-toggle="tab">
                    order by alphabet
                </a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane active" id="tabs-1" style="position:absolute;">
                <div class="container">
                    <div id="tags1">
                        <ul id="tag">
                        </ul>
                    </div>
                </div>
            </div>
            <div class="tab-pane" id="tabs-2" style="position:absolute;">
                <div class="container">
                    <div id="tags">
                        <ul id="tag-num">
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>

<script type="text/template" id="tag-template">	
   <a href='/searchpage<%= (user&&user.isPublic=='false') ? "/username/" + user.username : "" %>/tag/"<@= urlvalue @>"' title="<@= point @>" ><@= name @></a>(<@= num @>)
</script>

<link rel="stylesheet" type="text/css" href="/curation/css/screen.css" />
<script type="text/javascript" src="/curation/lib/bootstrap-tab.js"></script>
<script type="text/javascript" src="/curation/lib/jquery.tagcloud.min.js"></script>
<script type="text/javascript" src="/curation/lib/jquery.tinysort.min.js"></script>
<script type="text/javascript" src="/curation/js/alltags.js"></script>